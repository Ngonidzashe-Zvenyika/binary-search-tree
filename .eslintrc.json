{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "consistent-return": ["off"],
    "import/extensions": ["error", { "js": "always" }],
    "no-unused-expressions": ["error", { "allowTernary": true }]
  }
}
